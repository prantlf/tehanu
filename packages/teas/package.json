{
  "name": "tehanu-teas",
  "version": "0.0.1",
  "description": "Provides a set of assertion methods compatible with the built-in assert module usable in both Node.js and the browser.",
  "homepage": "https://github.com/prantlf/tehanu/tree/master/packages/teas#readme",
  "author": {
    "name": "Ferdinand Prantl",
    "email": "prantlf@gmail.com",
    "url": "http://prantl.tk"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/tehanu.git"
  },
  "bugs": {
    "url": "https://github.com/prantlf/tehanu/issues"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/tehanu/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "node build && cp index.d.ts dist",
    "test": "c8 teru test*"
  },
  "c8": {
    "include": [
      "index.js",
      "deep-compare.js"
    ],
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text"
    ],
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  },
  "devDependencies": {
    "@prantlf/esbuild": "^0.8.46",
    "c8": "^7.7.2",
    "tehanu": "workspace:*",
    "tehanu-repo-coco": "workspace:*",
    "tehanu-teru": "workspace:*"
  },
  "keywords": [
    "assert",
    "testing",
    "unit-testing",
    "test"
  ]
}
