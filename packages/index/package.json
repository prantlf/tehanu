{
  "name": "tehanu",
  "version": "0.2.1",
  "description": "Blazingly fast, tiny and simple JavaScript test harness for Node.js and browser.",
  "homepage": "https://github.com/prantlf/tehanu/tree/master/packages/index#readme",
  "author": {
    "name": "Ferdinand Prantl",
    "email": "prantlf@gmail.com",
    "url": "http://prantl.tk"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/tehanu.git"
  },
  "bugs": {
    "url": "https://github.com/prantlf/tehanu/issues"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/tehanu/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "node build && cp index.d.ts dist",
    "test": "c8 node -r source-map-support/register test"
  },
  "c8": {
    "include": [
      "index.js",
      "dist/index.js"
    ],
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text"
    ],
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  },
  "tehanu": {
    "autostart": false
  },
  "devDependencies": {
    "@prantlf/esbuild": "^0.8.46",
    "c8": "^7.11.0",
    "source-map-support": "^0.5.21",
    "tehanu-repo-tape": "workspace:*"
  },
  "keywords": [
    "test",
    "testing",
    "unit-testing",
    "test-framework",
    "test-harness",
    "test-runner"
  ]
}
