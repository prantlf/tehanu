{
  "name": "tehanu",
  "version": "0.0.1",
  "description": "Blazingly fast, tiny and simple JavaScript test harness for Node.js and browser.",
  "keywords": [],
  "author": "Ferdinand Prantl <prantlf@gmail.com>",
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "node build && cp index.d.ts dist",
    "test": "c8 node -r source-map-support/register test"
  },
  "c8": {
    "include": [
      "index.js",
      "dist/index.js"
    ],
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text"
    ],
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  },
  "tehanu": {
    "autostart": false
  },
  "devDependencies": {
    "@prantlf/esbuild": "^0.8.46",
    "c8": "^7.7.2",
    "source-map-support": "^0.5.19",
    "tehanu-repo-tape": "workspace:*"
  }
}
