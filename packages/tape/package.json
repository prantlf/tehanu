{
  "name": "tehanu-repo-tape",
  "version": "1.0.1",
  "description": "Reports the test progress of tests written with tehanu on the console using the TAP protocol format.",
  "homepage": "https://github.com/prantlf/tehanu/tree/master/packages/tape#readme",
  "author": {
    "name": "Ferdinand Prantl",
    "email": "prantlf@gmail.com",
    "url": "http://prantl.tk"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/tehanu.git"
  },
  "bugs": {
    "url": "https://github.com/prantlf/tehanu/issues"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/tehanu/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">=14.13"
  },
  "main": "dist/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
