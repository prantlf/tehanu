    "import": "./lib/index.mjs"
  },
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "prepare": "rollup -c && cp lib/index.d.ts dist"
  },
  "devDependencies": {
    "rollup": "^2.75.5",
    "rollup-plugin-swc-minify": "^1.0.2"
  },
  "keywords": [
    "tap",
